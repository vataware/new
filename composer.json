{
	"name": "vataware/new",
	"description": "The new vataware.com",
	"license": "GPL-2.0",
	"require": {
		"laravel/framework": "4.1.*",
		"bonroyage/messages": "dev-master",
		"skymeyer/vatsimphp": "dev-master",
		"halilim/xml-iterator": "dev-master",
		"iron-io/iron_mq": "dev-master",
		"iron-io/iron_cache": "dev-master",
		"dietervds/laravel4-ironcache": "dev-master",
		"philf/setting": "dev-master",
		"anahkiasen/flatten": "dev-master",
		"bonroyage/jira": "dev-master"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "1.*",
		"phpunit/phpunit": "3.7.*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "git@bitbucket.org:devosburchart/laravel-jira.git"
		}
	],
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/exceptions",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Vataware": "app/core"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
